MAKING RELEASE
  
  # edit version in textmap.py
  
  ./make_release textmap-0.2beta
  cp textmap-0.2beta.tgz textmap-0.2beta.zip CHANGELOG.txt ../../webpage/htm/
  
  # tag release
  git tag Release-0.2beta
  
  # then on webpage
  cd ../../webpage/htm
  git add textmap-0.2beta.tgz textmap-0.2beta.zip
